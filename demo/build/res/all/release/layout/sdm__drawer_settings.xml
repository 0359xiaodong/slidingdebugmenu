<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="horizontal"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginLeft="@dimen/sdm__drawer_side_padding"
              android:layout_marginRight="@dimen/sdm__drawer_side_padding"
              android:layout_alignParentBottom="true"
              android:gravity="center_horizontal"
              android:background="@drawable/sdm__drawer_settings_bg">

    <ImageButton
        style="@style/Widget.Sdm.Button.Transparent"
        android:id="@+id/sdm__settings_developer"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:src="@drawable/sdm__ic_developer"/>

    <ImageButton
        style="@style/Widget.Sdm.Button.Transparent"
        android:id="@+id/sdm__settings_battery"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:src="@drawable/sdm__ic_battery"/>

    <ImageButton
        style="@style/Widget.Sdm.Button.Transparent"
        android:id="@+id/sdm__settings_drawer"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:src="@drawable/sdm__ic_settings"/>

    <ImageButton
        style="@style/Widget.Sdm.Button.Transparent"
        android:id="@+id/sdm__settings_app_info"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:src="@drawable/sdm__ic_info"/>

    <ImageButton
        style="@style/Widget.Sdm.Button.Transparent"
        android:id="@+id/sdm__settings_uninstall"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:src="@drawable/sdm__ic_uninstall"/>
</LinearLayout><!-- From: file:/Users/balysv/AndroidStudioProjects/SlidingDebugMenu/library/src/main/res/layout/sdm__drawer_settings.xml --><!-- From: file:/Users/balysv/AndroidStudioProjects/SlidingDebugMenu/demo/build/exploded-bundles/SlidingDebugMenuLibraryUnspecified.aar/res/layout/sdm__drawer_settings.xml -->